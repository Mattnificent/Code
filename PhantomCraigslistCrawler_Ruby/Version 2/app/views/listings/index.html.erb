<h1>Listing listings</h1>

<table>
  <thead>
    <tr>
      <th>Userids</th>
      <th>Clistnumber</th>
      <th>Title</th>
      <th>Price</th>
      <th>Location</th>
      <th>Date</th>
      <th>Descriptionurl</th>
      <th>Pictureurls</th>
      <th>Sellersemail</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @listings.each do |listing| %>
      <tr>
        <td><%= listing.userIDs %></td>
        <td><%= listing.cListNumber %></td>
        <td><%= listing.title %></td>
        <td><%= listing.price %></td>
        <td><%= listing.location %></td>
        <td><%= listing.date %></td>
        <td><%= listing.descriptionURL %></td>
        <td><%= listing.pictureURLs %></td>
        <td><%= listing.sellersEmail %></td>
        <td><%= link_to 'Show', listing %></td>
        <td><%= link_to 'Edit', edit_listing_path(listing) %></td>
        <td><%= link_to 'Destroy', listing, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Listing', new_listing_path %>
